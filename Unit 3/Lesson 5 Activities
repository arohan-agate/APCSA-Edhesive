/* Lesson 5 Coding Activity Question 1 */

import java.util.Scanner;

public class U3_L5_Activity_One{
  public static void main(String[] args){

    Scanner scan = new Scanner(System.in);
    System.out.println("Enter 2 integers");
    
    int x = scan.nextInt();
    int y = scan.nextInt();
    
    if((x != 0) && (y/x >= 4 && y/x < 6))
      System.out.println("Ratio OK");

  }
}


/* Lesson 5 Coding Activity Question 2 */

import java.util.Scanner;

public class U3_L5_Activity_Two{
  public static void main(String[] args){

    Scanner scan = new Scanner(System.in);
    System.out.println("Enter two numbers");
    int a = scan.nextInt();
    int b = scan.nextInt();

    if (a != 0 && b % a == 0){
      System.out.println(b + " is a multiple of " + a);
    }else{
      System.out.println(b + " is not a multiple of " + a);
    }
  }
}
