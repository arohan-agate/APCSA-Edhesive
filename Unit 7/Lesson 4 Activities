import java.util.ArrayList;
public class U7_L4_Activity_One {
  public static int countInitial(ArrayList < String > list, String letter) {
    int count = 0;
    for (String word: list) {
      if (word.toLowerCase().startsWith(letter.toLowerCase())) {
        count++;
      }
    }
    return count;
  }
}


import java.util.ArrayList;
public class U7_L4_Activity_Two {
  public static int searchLast(ArrayList < String > objects, String target) {
    return objects.lastIndexOf(target);
  }
}
